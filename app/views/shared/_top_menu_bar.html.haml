/ Topbar Start
.navbar-custom
  
  %ul.list-unstyled.topbar-right-menu.float-right.mb-0

    %li.dropdown.notification-list
      %a.nav-link.dropdown-toggle.nav-user.arrow-none.mr-0{"aria-expanded" => "false", "aria-haspopup" => "false", "data-toggle" => "dropdown", :href => "#", :role => "button"}
        %span.account-user-avatar
          %img.rounded-circle{:alt => "user-image", :src => "#{asset_url('avatar-1.jpg')}"}
        %span
          %span.account-user-name
            Martin
          %span.account-position Founder
      .dropdown-menu.dropdown-menu-right.dropdown-menu-animated.topbar-dropdown-menu.profile-dropdown
        / item
        .dropdown-header.noti-title
          %h6.text-overflow.m-0 Welcome !
        / item
        %a.dropdown-item.notify-item{:href => "javascript:void(0);"}
          %i.mdi.mdi-account-circle.mr-1
          %span My Account
        / item
        %a.dropdown-item.notify-item{:href => "javascript:void(0);"}
          %i.mdi.mdi-account-edit.mr-1
          %span Settings
        / item
        %a.dropdown-item.notify-item{:href => "javascript:void(0);"}
          %i.mdi.mdi-lifebuoy.mr-1
          %span Support
        / item
        %a.dropdown-item.notify-item{:href => "javascript:void(0);"}
          %i.mdi.mdi-logout.mr-1
          %span Logout
  %button.button-menu-mobile.open-left.disable-btn
    %i#button-menu-mobile-toggle.mdi.mdi-menu

/ end Topbar
